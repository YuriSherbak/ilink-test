{
  "moduleFileExtensions": [
    "js",
    "json",
    "ts"
  ],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "libs/domain/(.*)": "<rootDir>/../libs/domain/src/$1",
    "libs/domain": "<rootDir>/../libs/domain/src"
  }
}